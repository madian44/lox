{
  "scopeName": "source.lox",
  "patterns": [{"include":  "#keywords"}, {"include":  "#strings"}],
  "repository": {
    "keywords": {
      "match": "\\b(and|class|else|false|for|fun|if|nil|or|print|return|super|this|true|var|while)\\b",
      "name": "keyword"
    },
    "strings": {
      "name": "string",
      "begin": "\"",
      "end": "\""
    }
  }
}